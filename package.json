{
  "name": "qwik-moviestracker",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloud-run/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "gcloud run deploy moviestracker-app --source .",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "bun server/entry.bun.js",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@auth/core": "^0.13.0",
    "@builder.io/qwik": "^1.3.0",
    "@builder.io/qwik-auth": "0.1.1",
    "@builder.io/qwik-city": "^1.3.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/compression": "^1.7.5",
    "@types/eslint": "8.56.2",
    "@types/node": "^20.11.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "autoprefixer": "^10.4.16",
    "bun-types": "^1.0.22",
    "daisyui": "^4.6.0",
    "eslint": "8.56.0",
    "eslint-plugin-qwik": "^1.3.5",
    "postcss": "^8.4.33",
    "prettier": "3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "5.3.3",
    "undici": "^6.3.0",
    "vite": "5.0.11",
    "vite-tsconfig-paths": "4.2.3"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^2.0.12",
    "@modular-forms/qwik": "^0.22.0",
    "@qwikest/icons": "^0.0.13",
    "@unpic/qwik": "^0.0.38",
    "bson": "^6.2.0",
    "dotenv": "^16.3.1",
    "firebase": "^10.7.1",
    "mongodb": "^6.3.0",
    "qwik-theme-toggle": "latest",
    "qwik-toasts": "latest",
    "typescript-cookie": "^1.0.6",
    "uuid": "^9.0.1",
    "valibot": "^0.25.0"
  }
}

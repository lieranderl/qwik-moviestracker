{
  "name": "moviestracker",
  "description": "Moviestracker",
  "engines": {
    "node": "^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/bun/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "bun server/entry.bun.js",
    "start": "vite --open --mode ssr",
    "biome": "biome check --fix",
    "qwik": "qwik"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.11.1",
    "daisyui": "^5.5.18",
    "@tailwindcss/typography": "latest",
    "@types/bun": "^1.3.9",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.19.11",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.3",
    "undici": "v7.16.0",
    "valibot": "1.1.0",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^5.1.4",
    "tailwindcss": "^4.2.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript-eslint": "8.38.0"
  },
  "devDependencies": {
    "@auth/qwik": "^0.9.1",
    "@biomejs/biome": "^2.4.2",
    "@builder.io/qwik": "^1.19.0",
    "@builder.io/qwik-city": "^1.19.0",
    "@catppuccin/daisyui": "^2.1.1",
    "@ditadi/qwik-hooks": "^0.1.6",
    "@modular-forms/qwik": "^0.29.1",
    "@qwikest/icons": "latest",
    "@tailwindcss/vite": "^4.2.0",
    "@unpic/qwik": "v1.0.0",
    "eslint-plugin-qwik": "^1.19.0",
    "mongodb": "^6.21.0",
    "qwik-theme-toggle": "^2.1.0",
    "qwik-toasts": "^2.0.2"
  }
}

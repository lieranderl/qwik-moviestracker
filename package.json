{
	"name": "moviestracker",
	"description": "Moviestracker",
	"engines": {
		"node": "^20.3.0 || >=21.0.0"
	},
	"engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
	"private": true,
	"trustedDependencies": ["sharp"],
	"trustedDependencies-annotation": "Needed for bun to allow running install scripts",
	"type": "module",
	"scripts": {
		"build": "qwik build",
		"build.client": "vite build",
		"build.preview": "vite build --ssr src/entry.preview.tsx",
		"build.server": "vite build -c adapters/bun/vite.config.ts",
		"build.types": "tsc --incremental --noEmit",
		"deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
		"dev": "vite --mode ssr",
		"dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
		"fmt": "prettier --write .",
		"fmt.check": "prettier --check .",
		"lint": "eslint \"src/**/*.ts*\"",
		"preview": "qwik build preview && vite preview --open",
		"serve": "bun server/entry.bun.js",
		"start": "vite --open --mode ssr",
		"biome": "biome check --fix",
		"qwik": "qwik"
	},
	"dependencies": {
		"@auth/mongodb-adapter": "^3.8.0",
		"daisyui": "^5.0.0",
		"@tailwindcss/typography": "latest",
		"@types/bun": "^1.2.4",
		"@types/eslint": "^8.56.12",
		"@types/node": "^22.13.8",
		"@typescript-eslint/eslint-plugin": "^8.25.0",
		"@typescript-eslint/parser": "^8.25.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.57.1",
		"prettier": "^3.5.2",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"typescript": "^5.8.2",
		"undici": "v7.4.0",
		"valibot": "v1.0.0-rc.1",
		"vite": "^5.4.14",
		"vite-tsconfig-paths": "^5.1.4",
		"tailwindcss": "^4.0.9",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@auth/qwik": "^0.6.0",
		"@biomejs/biome": "^1.9.4",
		"@builder.io/qwik": "^1.12.1",
		"@builder.io/qwik-auth": "^0.2.2",
		"@builder.io/qwik-city": "^1.12.1",
		"@ditadi/qwik-hooks": "^0.1.6",
		"@modular-forms/qwik": "^0.29.1",
		"@qwikest/icons": "latest",
		"@tailwindcss/vite": "^4.0.9",
		"@unpic/qwik": "v1.0.0",
		"eslint-plugin-qwik": "^1.12.1",
		"mongodb": "^6.14.0",
		"qwik-theme-toggle": "^2.0.4",
		"qwik-toasts": "^2.0.2"
	}
}
